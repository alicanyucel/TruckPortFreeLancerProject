{
  "name": "truck-port",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "build:dev": "ng build --configuration=development",
    "build:prod": "ng build --configuration=production --optimization=true --build-optimizer=true --aot=true --vendor-chunk=true --common-chunk=true --source-map=false",
    "build:analyze": "ng build --configuration=production --stats-json && npx webpack-bundle-analyzer dist/truck-port/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "dev:ssr": "ng run TruckPort:serve-ssr",
    "serve:ssr": "node dist/TruckPort/server/main.js",
    "build:ssr": "ng build && ng run TruckPort:server",
    "build:ssr:prod": "ng build --configuration=production && ng run TruckPort:server:production",
    "prerender": "ng run TruckPort:prerender",
    "docker:build": "docker build -t truckport:latest .",
    "docker:run": "docker run -p 80:80 truckport:latest",
    "docker:compose": "docker-compose up -d",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "performance:lighthouse": "lighthouse http://localhost:4200 --output=json --output-path=./lighthouse-report.json",
    "performance:bundlesize": "bundlesize",
    "json-server": "json-server --watch db.json --port 3002",
    "api": "json-server --watch db.json --port 3002"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/fire": "^16.0.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/platform-server": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.2.0",
    "@ngrx/store": "^16.3.0",
    "@ngrx/store-devtools": "^16.3.0",
    "@nguniversal/express-engine": "^16.2.0",
    "express": "^4.15.2",
    "firebase": "^10.14.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/cli": "^16.2.16",
    "@angular/compiler-cli": "^16.2.0",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "jasmine-core": "~4.6.0",
    "json-server": "^1.0.0-beta.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3"
  }
}
